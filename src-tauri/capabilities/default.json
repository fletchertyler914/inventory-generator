{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    {
      "identifier": "opener:allow-open-path",
      "allow": [
        {
          "path": "**"
        }
      ]
    },
    "dialog:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "sql:default",
    "sql:allow-execute",
    "sql:allow-select",
    "sql:allow-load"
  ]
}
